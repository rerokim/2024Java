package sec01.exam02;

public class e02IfElse {
	public static void main(String[] args) {
		int score = 85;
		
		if(score>=90) {
			System.out.println("정수가 90보다 큽니다.");
			System.out.println("등급은 A입니다.");
		} else {	//score<90 일 경
			System.out.println("정수가 90보다 작습니다. ");
			System.out.println("등급은 B입니다. ");
		}
	}
}
